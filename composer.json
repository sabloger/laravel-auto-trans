{
    "name": "sabloger/laravel-auto-trans",
    "description": "Convert Hard-Stringed blades to auto-generated Multi-language blades! And only you need translate language files for other languages!",
    "type": "package",
    "require": {
        "laravel/laravel": "^5.0"
    },
    "require-dev": {
        "laravel/laravel": "^5.0",
        "phpunit/phpunit": "^6.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Saeed Falsafin",
            "email": "sabloger@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "AutoTrans\\": "packages/sabloger/laravel-auto-trans"
        }
    }
}
